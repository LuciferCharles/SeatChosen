.container{
  width: 100%;
}
.head{
  display: flex;
  justify-content: center;
}
.head text{
  height: 2.5rem;
  line-height: 2.5rem;
  width: 16%;
  text-align: center;
}
.detail{
  width: 100%;
  height: 900rpx;
  border: solid 1px skyblue;
}
.detail_item{
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-top: 5px;
}
.room_item{
  display: flex;
  flex-direction: row;
  justify-content: center;
  border: solid 1px skyblue;
  margin-top: 5px;
}
.room_item text{
  width: 16%;
  text-align: center;
  font-size: 30rpx;
}


.link{
  width: 16%;
  text-align: center;
  color: blue;
  display: inline-table;
}

